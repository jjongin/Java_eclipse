public class Calculator extends JFrame {
	public class Calculator {
	
		private JPanel panel;
		private JTextField tField;
		private JButton[] buttons;
		private String[] labels = {
		"Backspace", "", "", "CE", "C",
		"7", "8", "9", "/", "sqrt",
		"4", "5", "6", "x", "%",
		"1", "2", "3", "-", "1/x",
		"0", "+/-", ".", "+", "=",
		};
		public Calculator() {
		tField = new JTextField(35);
		panel = new JPanel();
